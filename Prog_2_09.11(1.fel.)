import math
def max(a):
    max=0
    for i in range(len(a)):
        if a[i]>max:
            max=a[i]
    return max


def min(a):
    min=math.inf
    for i in range(len(a)):
        if a[i]<min:
            min=a[i]
    return min
    
    def main():
    a=(1,4,2,10,44,5)
    print("Maximum: ", max(a))
    print("Minimum: ", min(a))
main()
